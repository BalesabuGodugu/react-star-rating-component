{
  "name": "react-star-rating-component",
  "version": "0.1.1",
  "description": "React component for star (or any other icon based) ratings",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src/StarRatingComponent.jsx --out-file index.js",
    "build-dev": "browserify ./example/app.js -o ./example/bundle.js -t [ babelify --presets [ es2015 stage-1 react ] ]",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voronianski/react-star-rating-component.git"
  },
  "keywords": [
    "react",
    "component",
    "star",
    "rating",
    "rate",
    "input"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "react"
    ]
  },
  "author": "Dmitri Voronianski <dmitri.voronianski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voronianski/react-star-rating-component/issues"
  },
  "homepage": "https://github.com/voronianski/react-star-rating-component#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "react": "^0.14.x",
    "react-dom": "^0.14.x"
  },
  "dependencies": {
    "classnames": "^2.2.0"
  }
}
